                        <script>
                            jQuery(function($) {
                                window.setTimeout(function() {
                                    $('#generatorfinder').slideDown(600);
                                }, 1400);

                                $('#conditionpicker').click(function() {
                                    $('#cpop').slideToggle()
                                });
                                $('.gfpc').click(function() {
                                    $('#gfvalc').text($(this).text());
                                    $('#gfinputc').val($(this).attr('data-val'));
                                });
                                $('#cpop').click(function() {
                                    $(this).slideToggle()
                                });

                                $('#fuelpicker').click(function() {
                                    $('#fpop').slideToggle()
                                });
                                $('.gfpf').click(function() {
                                    $('#gfvalf').text($(this).text());
                                    $('#gfinputf').val($(this).attr('data-val'));
                                });
                                $('#fpop').click(function() {
                                    $(this).slideToggle()
                                });

                                $('#wattpicker').click(function() {
                                    $('#wpop').slideToggle()
                                });
                                $('.gfpw').click(function() {
                                    $('#gfvalw').text($(this).text());
                                    $('#gfinputwmin').val($(this).attr('data-valmin'));
                                    $('#gfinputwmax').val($(this).attr('data-valmax'));
                                });
                                $('#wpop').click(function() {
                                    $(this).slideToggle()
                                });

                                $('#gfsubmitbtn').click(function() {
                                    $('#genfinderform').submit()
                                });
                            });
